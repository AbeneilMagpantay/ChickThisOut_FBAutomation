{
    "version": 2,
    "builds": [
        {
            "src": "api/webhook.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/webhook",
            "dest": "/api/webhook.py"
        },
        {
            "src": "/api/health",
            "dest": "/api/webhook.py"
        },
        {
            "src": "/(.*)",
            "dest": "/api/webhook.py"
        }
    ]
}